<div class="header-form">
    <!-- <p class="mrgn-clear font-size-12 font-weg-600 disabled-text-1 mrgn-r-10">Belum punya akun?</p><button type="button" class="btn-filled solid morp-1" data-btnlabel="Sign Up"></button> -->
</div>
<div class="content-form">
    <div class="company-logo dsp-flex" style="border: 1px solid var(--third-color); width: fit-content; border-radius: 50%; padding: 4px;">
        <img src="img/logos/teamreport-logo-gradient2.png" alt="" style="margin: 0;">
    </div>
    <p class="font-size-13 font-weg-600 disabled-text-1 mrgn-clear">Signup</p>
    <h1 class="title">Masukan Informasi Personal</h1>
    <div class="box dsp-flex fl-colm gap-16">

        <div class="input-field default icon" data-validatormode="offline" data-type="number" tabindex="0" style="width: 100%; max-width: 500px;">
            <div class="icons8-regular pincode"></div>
            <input name="form_user" maxlength="16" id="" aria-label="Nomor Induk Kependudukan" autocomplete="off"/>
            <span data-msginput=" / Cannot be empty!">Nomor Induk Kependudukan</span>
            <span class="show-pass"></span>
        </div>

        <div class="box dsp-flex gap-10">
            <div class="input-field default icon" data-validatormode="offline" data-type="text" tabindex="0" style="width: 100%; max-width: 500px;">
                <div class="icons8-regular ball-point-pen"></div>
                <input name="form_user" id="" aria-label="Nama Depan" autocomplete="off"/>
                <span data-msginput=" / Cannot be empty!">Nama Depan</span>
                <span class="show-pass"></span>
            </div>
            <div class="input-field default icon" data-validatormode="offline" data-type="text" tabindex="0" style="width: 100%; max-width: 500px;">
                <div class="icons8-regular ball-point-pen"></div>
                <input name="form_user" id="" aria-label="Nama Belakang" autocomplete="off"/>
                <span data-msginput=" / Cannot be empty!">Nama Belakang</span>
                <span class="show-pass"></span>
            </div>
        </div>

        <div class="box dsp-flex gap-10">
            <div class="input-field default icon" data-validatormode="offline" data-type="text" tabindex="0" style="width: 100%; max-width: 500px;">
                <div class="icons8-regular user"></div>
                <input name="form_user" id="" aria-label="Username" autocomplete="off"/>
                <span data-msginput=" / Cannot be empty!">Username</span>
                <span class="show-pass"></span>
            </div>

            <div class="input-field default icon" data-validatormode="offline" data-type="password" tabindex="0" style="width: 100%; max-width: 500px;">
                <div class="icons8-regular password"></div>
                <input name="form_user" id="" aria-label="Password" autocomplete="off"/>
                <span data-msginput=" / Cannot be empty!">Password</span>
                <span class="show-pass"></span>
            </div>
        </div>
        <div class="box dsp-flex gap-10">
            <div class="input-field default icon" data-validatormode="offline" data-type="email" tabindex="0" style="width: 100%; max-width: 500px;">
                <div class="icons8-regular mail"></div>
                <input name="form_user" id="" aria-label="Email" autocomplete="off"/>
                <span data-msginput=" / Cannot be empty!">Email</span>
                <span class="show-pass"></span>
            </div>

            <div class="input-field default icon" data-validatormode="offline" data-type="date" tabindex="0" style="width: 100%; max-width: 500px;">
                <div class="icons8-regular calendar"></div>
                <input name="form_user" id="" aria-label="Tanggal Lahir" autocomplete="off"/>
                <span data-msginput=" / Cannot be empty!">Tanggal Lahir</span>
                <span class="show-pass"></span>
            </div>
        </div>
    </div>
    <div class="end-input">
        <div class="end-input">
            <div class="box dsp-flex justify-end">
                <button type="button" style="width: 150px;" id="next" class="btn-filled default-gradient" onclick="openPage('/signup/lokasialamat', 'multiple-form', 'bottom')" data-btnlabel="Lanjut"></button>
            </div>
        </div>
    </div>
</div>
<div class="footer-form">
    <a href="/masyarakat/help" class="hyperlink">
        <div class="icons8-filled inquiry"></div>Butuh bantuan?
    </a>
</div>